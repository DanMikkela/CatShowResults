@model IEnumerable<Cat_Show_Results.Models.Ticket>

@{
    ViewData["Title"] = "Results List";
}

<h4>Results List</h4>

@*<p>
    <a asp-action="Create">Create New</a>
</p>*@
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Type)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Head)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Eyes)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Ears)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Fur)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Tail)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Condition)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.General)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Result)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.JudgeName)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Cat)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Type)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Head)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Eyes)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Ears)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Fur)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Tail)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Condition)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.General)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Result)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.JudgeName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cat.CatId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.TicketId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.TicketId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.TicketId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
